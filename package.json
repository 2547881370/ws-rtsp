  "devDependencies": {
    "@types/node": "^10.12.9",
    "@types/socket.io": "^2.1.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@types/atob": "^2.1.2",
    "@types/ws": "^7.4.0",
